{
  "name": "vue-maptalks-tool",
  "type": "module",
  "version": "0.0.7",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:v2": "VITE_VUE_VERSION=2 vite build --mode v2",
    "build:v3": "VITE_VUE_VERSION=3 vite build --mode v3",
    "preview": "vite preview --port 4173",
    "doc": "jsdoc --readme README.md -c jsdoc.json"
  },
  "dependencies": {
    "maptalks": "^1.0.0-rc.17",
    "vue": "^2.7.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "docdash": "^2.0.0",
    "jsdoc": "^4.0.0",
    "vite": "^3.0.2",
    "vite-plugin-libcss": "^1.0.5"
  },
  "files": [
    "dist"
  ],
  "description": "vue maptalks tool",
  "main": "./dist/vue-maptalks-tool.umd.cjs",
  "module": "./dist/vue-maptalks-tool.js",
  "exports": {
    ".": {
      "import": "./dist/vue-maptalks-tool.js",
      "require": "./dist/vue-maptalks-tool.umd.cjs"
    }
  },
  "author": "tim760255458",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim760255458/vue-maptalks-tool"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tim760255458/vue-maptalks-tool/issues"
  },
  "homepage": "https://tim760255458.github.io/vue-maptalks-tool/index.html",
  "pnpm": {
    "patchedDependencies": {
      "docdash@2.0.0": "patches/docdash@2.0.0.patch"
    }
  }
}
